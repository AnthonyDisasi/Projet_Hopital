{% extends 'base.html.twig' %}

{% block title %}Espace Infirmier{% endblock %}

{% block navbar %}
    <ul>
        <li class="navbar-brand">
            Infirmier
        </li>
    </ul>
{% endblock %}
{% block body %}

    <table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Date traitement</th>
            <th scope="col">Date fin taitment</th>
        </tr>
    </thead>
    <tbody>
    {% for traitement in traitements  %}
            <tr>
                <th scope="row">{{ traitement.id }}</th>
                <td>{{ traitement.datetraitement|date('d/m/Y') }}</td>
                <td>{{ traitement.datefintraitement|date('d/m/Y') }}</td>
            </tr>
    {% endfor %}
    </tbody>
    </table>
{% endblock %}